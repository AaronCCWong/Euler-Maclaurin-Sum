/*
 * This program calculates \log( \gamma ( 1-s ) )
 * where s = sigma + i * t.
 */

#include <iostream>
#include <gmp.h>
#include <mpfr.h>
using namespace std;

int main() {
  int mpfr_bits = 300;
  // initialize mpfr variables
  mpfr_t sigma, t, arg;
  mpfr_init(sigma,mpfr_bits);
  mpfr_init(t,mpfr_bits);
  mpfr_set_d(sigma, 0.5, GMP_RNDN);
  mpfr_set_ui(t, 1000000, GMP_RNDN);
  
  // clear mprf variables
  mpfr_clear(sigma);
  mpfr_clear(t);
  return 0;
}

void argument(mpfr_t sigma, mpfr_t t, mpfr_t arg) {
}

